<?xml version="1.0" encoding="UTF-8"?>
<manifest version="0.2" xml:base="default/">
    <template name="default">
        <author>Mike de Boer</author>
        <country>NL</country>
        <creationdate>11/26/2007</creationdate>
        <description>Default theme of zOOm Media Gallery</description>
        <ranking>100.0</ranking>
    </template>
    <views>
        <view name="gallery:overview"/>
        <view name="gallery:search"/>
        <view name="gallery:special"/>
        <view name="gallery:lightbox"/>
        <view name="gallery:thumbnails"/>
        <view name="gallery:thumbnails:medium"/>
        <view name="gallery:thumbnails:medium:edit"/>
        <view name="gallery:thumbnails:medium:delete"/>
        <view name="gallery:thumbnails:medium:lightbox"/>
        <view name="useradmin"/>
        <view name="singlepage" inherits="gallery:*,useradmin"/>
    </views>
    <resources xml:base="default/">
        <resource name="stylesheet" xml:base="css/">
            <file href="zmg_default.css"/>
        </resource>
        <resource name="javascript" xml:base="js/">
            <file href="zmg_default.js" />
        </resource>
        <resource name="template" type="html" view="singlepage" xml:base="">
            <file href="default.html.tpl"/>
        </resource>
        <resource name="preview" xml:base="images/">
            <file href="default_preview.png"/>
        </resource>
    </resources>
</manifest>